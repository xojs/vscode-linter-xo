{
  "name": "linter-xo",
  "version": "2.2.0",
  "description": "Linter for XO",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SamVerschueren/vscode-linter-xo"
  },
  "author": {
    "name": "Sam Verschueren",
    "email": "sam.verschueren@gmail.com",
    "url": "github.com/SamVerschueren"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "compile": "installServerIntoExtension ../xo ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../xo ./package.json ./tsconfig.json && tsc --watch -p .",
    "lint": "tslint -p . --format stylish"
  },
  "dependencies": {
    "load-json-file": "^2.0.0",
    "vscode-languageserver": "^4.1.3"
  },
  "devDependencies": {
    "@types/load-json-file": "^2.0.5",
    "@types/node": "^8.10.18",
    "tslint": "^5.10.0",
    "tslint-xo": "^0.8.0",
    "typescript": "^2.8.3"
  }
}
